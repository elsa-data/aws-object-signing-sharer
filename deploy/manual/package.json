{
  "name": "manual",
  "private": true,
  "version": "0.0.0",
  "description": "Manual CDK deployment for UMCCR and AG",
  "scripts": {
    "dev-deploy": "cdk --app 'npx ts-node stacks.ts' deploy ElsaDataDevObjectSigningStack",
    "dev-destroy": "cdk --app 'npx ts-node stacks.ts' destroy ElsaDataDevObjectSigningStack",
    "agdemo-deploy": "cdk --app 'npx ts-node stacks.ts' deploy ElsaDataDemoAustralianGenomicsObjectSigningStack",
    "agdemo-destroy": "cdk --app 'npx ts-node stacks.ts' destroy ElsaDataDemoAustralianGenomicsObjectSigningStack",
    "ag-deploy": "cdk --app 'npx ts-node stacks.ts' deploy ElsaDataAustralianGenomicsObjectSigningStack",
    "ag-destroy": "cdk --app 'npx ts-node stacks.ts' destroy ElsaDataAustralianGenomicsObjectSigningStack",
    "doctor": "cdk --app 'npx ts-node stacks.ts' doctor ElsaDataLocalDevTestObjectSigningStack",
    "synth": "cdk --app 'npx ts-node stacks.ts' synth ElsaDataLocalDevTestObjectSigningStack"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.0",
    "@types/node": "18.17.3",
    "prettier": "3.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "aws-cdk": "2.90.0",
    "aws-cdk-lib": "2.90.0",
    "cdk-nag": "^2.27.93",
    "workload-object-signing": "file:../../workload-object-signing"
  }
}
